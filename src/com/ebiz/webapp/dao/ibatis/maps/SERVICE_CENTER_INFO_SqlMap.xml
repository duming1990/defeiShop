	<select id="selectServiceCenterInfoPaiMingList" resultMap="ServiceCenterInfoPaiMing" parameterClass="serviceCenterInfo" cacheModel="oneDayCache">
					SELECT f.FULL_NAME,e.SERVICECENTER_NAME,d.* from (SELECT
				c.P_INDEX, count(1) entp_count, sum(entp_sale_money) entp_sale_money
			FROM
				(
					SELECT a.* FROM (
							SELECT
								t.entp_id,
								b.ENTP_NAME, 
								b.P_INDEX,
								sum(t.order_money) AS entp_sale_money
							FROM
								order_info t
							LEFT JOIN entp_info b ON t.entp_id = b.id
							WHERE  
							t.ORDER_STATE >= 10 
							AND
							t.ORDER_TYPE IN (10, 11, 20, 40, 70)
						  <!--根据时间查询 -->
						  <isNotEmpty prepend=" and " property="map.sereach_servecenter_st_date">
				          <![CDATA[t.add_date >= str_to_date(#map.sereach_servecenter_st_date:VARCHAR#, '%Y-%m-%d')]]></isNotEmpty>                     
				          <isNotEmpty prepend=" and " property="map.sereach_servecenter_en_date">
				          <![CDATA[t.add_date < str_to_date(#map.sereach_servecenter_en_date:VARCHAR#, '%Y-%m-%d')]]></isNotEmpty> 
				          <!--  根据区域查询条件 -->
				          <isNotEmpty prepend=" and " property="map.p_index_like">b.P_INDEX like #map.p_index_like#  '%' </isNotEmpty>
							GROUP BY t.ENTP_ID
						) a
					
				) c 
			GROUP BY c.P_INDEX) d  
			
			<isNotEmpty property="map.left_join">left join</isNotEmpty>
			<isNotEmpty property="map.right_join">right join</isNotEmpty>
			service_center_info e on d.P_INDEX = e.P_INDEX 
			
			LEFT JOIN base_province f on d.P_INDEX = f.P_INDEX
<!-- 	<include refid="sf-serviceCenterInfo" /> -->
             where 1=1
            <isNotEmpty prepend=" and " property="map.add_user_id">e.ADD_USER_ID = #map.add_user_id:INTEGER#</isNotEmpty>
            <isNotEmpty prepend=" and " property="map.id">e.ID = #map.id:INTEGER#</isNotEmpty>
            <isNotEmpty prepend=" and " property="map.is_del">e.IS_DEL = #map.is_del:INTEGER#</isNotEmpty>
            <isNotEmpty prepend=" and " property="map.effect_state">e.EFFECT_STATE = #map.effect_state:INTEGER#</isNotEmpty>
			order by d.entp_sale_money desc
		<isNotEmpty property="row.count">
			limit #row.first#, #row.count#
		</isNotEmpty>
	</select>
	
