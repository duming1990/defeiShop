<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="BASE_ATTRIBUTE">

	<typeAlias alias="baseAttribute" type="com.ebiz.webapp.domain.BaseAttribute" />

	<cacheModel id="oneDayCache" type="OSCACHE">
		<flushInterval hours="24" />
		<flushOnExecute statement="insertBaseAttribute" />
		<flushOnExecute statement="updateBaseAttribute" />
		<flushOnExecute statement="deleteBaseAttribute" />
	</cacheModel>

	<resultMap id="baseAttributeResultForList" class="baseAttribute">
		<result column="ID" property="id" jdbcType="INTEGER" />
		<result column="ATTR_SCOPE" property="attr_scope" jdbcType="INTEGER" />
		<result column="ATTR_NAME" property="attr_name" jdbcType="VARCHAR" />
		<result column="ATTR_SHOW_NAME" property="attr_show_name" jdbcType="VARCHAR" />
		<result column="TYPE" property="type" jdbcType="INTEGER" />
		<result column="REMARK" property="remark" jdbcType="VARCHAR" />
		<result column="IS_REQUIRED" property="is_required" jdbcType="INTEGER" />
		<result column="IS_SHOW" property="is_show" jdbcType="INTEGER" />
		<result column="IS_BRAND" property="is_brand" jdbcType="INTEGER" />
		<result column="ORDER_VALUE" property="order_value" jdbcType="INTEGER" />
		<result column="IS_LOCK" property="is_lock" jdbcType="INTEGER" />
		<result column="IS_DEL" property="is_del" jdbcType="INTEGER" />
		<result column="ADD_DATE" property="add_date" jdbcType="TIMESTAMP" />
		<result column="ADD_USER_ID" property="add_user_id" jdbcType="INTEGER" />
