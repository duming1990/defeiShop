		<result column="UPDATE_USER_ID" property="update_user_id" jdbcType="INTEGER" />
		<result column="DEL_DATE" property="del_date" jdbcType="TIMESTAMP" />
		<result column="DEL_USER_ID" property="del_user_id" jdbcType="INTEGER" />
		<result column="IS_SHOW_PIC" property="is_show_pic" jdbcType="INTEGER" />
		<result column="LINK_ID" property="link_id" jdbcType="INTEGER" />
		<result column="OWN_ENTP_ID" property="own_entp_id" jdbcType="INTEGER" />
		<result column="CLS_ID" property="cls_id" jdbcType="INTEGER" />
		<result column="LINK_HAS_ATTR_ID" property="link_has_attr_id" jdbcType="INTEGER" />
		<result column="IS_WORK" property="is_work" jdbcType="INTEGER" />
	</resultMap>

	<resultMap id="baseAttributeResult" class="baseAttribute" extends="baseAttributeResultForList">
	</resultMap>

	<sql id="sf-baseAttribute">
		<isNotEmpty prepend=" and " property="id">a.ID = #id:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="attr_scope">a.ATTR_SCOPE = #attr_scope:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="attr_name">a.ATTR_NAME = #attr_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="attr_show_name">a.ATTR_SHOW_NAME = #attr_show_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="type">a.TYPE = #type:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="remark">a.REMARK = #remark:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_required">a.IS_REQUIRED = #is_required:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_show">a.IS_SHOW = #is_show:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_brand">a.IS_BRAND = #is_brand:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="order_value">a.ORDER_VALUE = #order_value:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_lock">a.IS_LOCK = #is_lock:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_del">a.IS_DEL = #is_del:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_date">a.ADD_DATE = #add_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_user_id">a.ADD_USER_ID = #add_user_id:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="add_user_name">a.ADD_USER_NAME = #add_user_name:VARCHAR#</isNotEmpty>
		<isNotEmpty prepend=" and " property="update_date">a.UPDATE_DATE = #update_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="update_user_id">a.UPDATE_USER_ID = #update_user_id:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="del_date">a.DEL_DATE = #del_date:TIMESTAMP#</isNotEmpty>
		<isNotEmpty prepend=" and " property="del_user_id">a.DEL_USER_ID = #del_user_id:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_show_pic">a.IS_SHOW_PIC = #is_show_pic:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="link_id">a.LINK_ID = #link_id:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="own_entp_id">a.OWN_ENTP_ID = #own_entp_id:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="cls_id">a.CLS_ID = #cls_id:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="link_has_attr_id">a.LINK_HAS_ATTR_ID = #link_has_attr_id:INTEGER#</isNotEmpty>
		<isNotEmpty prepend=" and " property="is_work">a.IS_WORK = #is_work:INTEGER#</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.attr_name_like">a.ATTR_NAME LIKE '%'  #map.attr_name_like:VARCHAR#  '%' </isNotEmpty>
		<isNotEmpty prepend=" and " property="map.attr_show_name_like">a.ATTR_SHOW_NAME LIKE '%'  #map.attr_show_name_like:VARCHAR#  '%' </isNotEmpty>
		<isNotEmpty prepend=" and " property="map.id_in">
		id in 
		(select attr_id from base_class_link_attribute where cls_id = #map.cls_id#)
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.id_not_in">
		id not in 
		(select attr_id from base_class_link_attribute where cls_id = #map.cls_id#)		
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.link_has_attr_id_not_in">
		id not in ($map.link_has_attr_id_not_in$)		
		</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.link_has_attr_id_not_null">
		 link_has_attr_id is not null
		</isNotEmpty>
		
		<isNotEmpty prepend=" and " property="map.left_join_baseAttrSon">
		a.id in (select t2.attr_id from base_attribute_son  t2 
		<isNotEmpty prepend=" " property="map.attr_ids_son"> where t2.id  in 
			<iterate close=")" open="(" conjunction="," property="map.attr_ids_son">#map.attr_ids_son[]#</iterate>
		</isNotEmpty>
		)
		</isNotEmpty>
		
		
		<isNotEmpty prepend=" and " property="map.attr_ids">
			a.ID in
			 <iterate close=")" open="(" conjunction="," property="map.attr_ids">#map.attr_ids[]#</iterate>
		 </isNotEmpty>
	</sql>

	<select id="selectBaseAttribute" resultMap="baseAttributeResult" parameterClass="baseAttribute" cacheModel="oneDayCache">
		select * from BASE_ATTRIBUTE a where 1 = 1
		<include refid="sf-baseAttribute" />
	</select>

	<select id="selectBaseAttributeList" resultMap="baseAttributeResultForList" parameterClass="baseAttribute" cacheModel="oneDayCache">
		
		select * from BASE_ATTRIBUTE a 
		
		<isNotEmpty prepend=" " property="map.cls_link_attr">left join base_class_link_attribute b on a.id = b.attr_id</isNotEmpty>
		where 1 = 1
		<isNotEmpty prepend=" and " property="map.cls_link_attr">b.cls_id = #map.cls_link_attr:INTEGER#</isNotEmpty>
		<include refid="sf-baseAttribute" />
		order by a.ORDER_VALUE desc, a.ID
		<isNotEmpty property="row.count">
			limit 0, #row.count#
		</isNotEmpty>
	</select>

	<select id="selectBaseAttributeCount" resultClass="int" parameterClass="baseAttribute" cacheModel="oneDayCache">
		select count(*) from BASE_ATTRIBUTE a where 1 = 1
		<include refid="sf-baseAttribute" />
	</select>

	<select id="selectBaseAttributePaginatedList" resultMap="baseAttributeResult" parameterClass="baseAttribute" cacheModel="oneDayCache">
		
		select * from BASE_ATTRIBUTE a where 1 = 1
		<include refid="sf-baseAttribute" />
		<!-- order by ID asc -->
		<isNotEmpty property="row.count">limit #row.first#, #row.count#</isNotEmpty>
	</select>

