		delete from BASE_ATTRIBUTE where 1 = 1
		<isNotEmpty prepend=" and " property="id">ID = #id#</isNotEmpty>
		<isNotEmpty prepend=" and " property="map.own_entp_id">OWN_ENTP_ID = #map.own_entp_id#</isNotEmpty>
